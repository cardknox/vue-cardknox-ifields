{
  "name": "@cardknox/vue-cardknox-ifields",
  "version": "2.2.55",
  "description": "A Vue component for Cardknox iFields",
  "main": "dist/vue-cardknox-ifields.umd.js",
  "module": "dist/vue-cardknox-ifields.esm.js",
  "unpkg": "dist/vue-cardknox-ifields.min.js",
  "browser": {
    "./sfc": "src/ifields/ifields.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.mjs --format umd --file dist/vue-cardknox-ifields.umd.js",
    "build:es": "rollup --config rollup.config.mjs --format es --file dist/vue-cardknox-ifields.esm.js",
    "build:unpkg": "rollup --config rollup.config.mjs --format iife --file dist/vue-cardknox-ifields.min.js",
    "start": "rollup --config rollup.config.mjs --format es --file dist/vue-cardknox-ifields.esm.js --watch"
  },
  "keywords": [
    "vue",
    "cardknox",
    "ifields",
    "payment",
    "credit card",
    "gateway"
  ],
  "bugs": {
    "url": "https://www.cardknox.com/contact/",
    "email": "developersupport@cardknox.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cardknox/vue-cardknox-ifields.git"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^25.0.4",
    "rollup": "^3.28.1",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.3.4",
    "vue-template-compiler": "^2.7.14"
  }
}
